cd ~/Desktop
rm -rf MyAssistantOS-Raspbian
git clone -branch LTS1.1p1 https://github.com/mytja/MyAssistantOS-Raspbian/
cd MyAssistantOS-Raspbian
chmod +x updater
mv updater ~/Desktop/
sudo apt-get install festival
sudo apt install python-pip
chmod +x fix_oobe
python -m pip install SpeechRecognition --user
python -m pip install talkey --user
python -m pip install pyowm --user
python -m pip install PocketSphinx --user
python -m pip install PyAudio --user
python -m pip install wikipedia --user
python -m pip install playsound --user
python -m pip install vlc --user
python -m pip install urllib --user
python -m pip install pafy --user
python -m pip install youtube-search-python --user
python -m pip install youtube-dl --user
cd ~/Desktop
touch OWM_license.txt
cd MyAssistantOS-Raspbian
python my.py
